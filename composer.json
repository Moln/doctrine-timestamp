{
    "name": "mmerian/doctrine-timestamp",
    "type": "library",
    "description": "A library adding the timestamp type to Doctrine ORM",
    "keywords": ["Doctrine", "ORM", "timestamp", "DateTime"],
    "license": "MIT",
    "authors": [
        {
            "name": "Moln Xie",
            "email": "moln.xie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "doctrine/dbal": "2.* | ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "DoctrineTimestamp\\DBAL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "DoctrineTimestamp\\Test\\DBAL\\Types\\": "tests/" }
    }
}